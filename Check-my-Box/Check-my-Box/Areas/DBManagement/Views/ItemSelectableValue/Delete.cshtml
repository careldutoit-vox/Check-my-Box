@model Check_my_Box.Areas.DBManagement.Models.ItemSelectableValue

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>ItemSelectableValue</legend>

    <div class="display-label">PlaceHolder</div>
    <div class="display-field">@Model.PlaceHolder</div>

    <div class="display-label">ListOptionValue</div>
    <div class="display-field">@Model.ListOptionValue</div>

    <div class="display-label">ItemSelectable</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.ItemSelectable).ToString()</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


