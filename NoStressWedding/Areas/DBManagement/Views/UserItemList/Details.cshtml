@model NoStressWedding.Areas.DBManagement.Models.UserItemList

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>UserItemList</legend>

    <div class="display-label">UserAvailable</div>
    <div class="display-field">@(Model.UserAvailable == null ? "None" : Model.UserAvailable.Count.ToString())</div>

    <div class="display-label">UserItems</div>
    <div class="display-field">@(Model.UserItems == null ? "None" : Model.UserItems.Count.ToString())</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.UserItemListId }) |
    @Html.ActionLink("Back to List", "Index")
</p>


